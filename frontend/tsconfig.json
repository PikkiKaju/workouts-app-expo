{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./src/*"],
      "app/*": ["./src/app/*"],
      "assets/*": ["./src/assets/*"],
      "components/*": ["./src/components/*"],
      "constants/*": ["./src/constants/*"],
      "data/*": ["./src/data/*"],
      "hooks/*": ["./src/hooks/*"],
      "models/*": ["./src/models/*"],
      "providers/*": ["./src/providers/*"],
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
