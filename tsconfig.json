{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "paths": {
      "@/*": ["./*"],
      "app/*": ["./app/*"],
      "assets/*": ["./assets/*"],
      "components/*": ["./components/*"],
      "constants/*": ["./constants/*"],
      "data/*": ["./data/*"],
    },
    "esModuleInterop": true,
    "resolveJsonModule": true
  }
}
